{
  "name": "@tool-123/ni-cli",
  "version": "0.0.3",
  "description": "Use the right package manager",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bookmarkbao/tool-123.git"
  },
  "author": "da xiang <bookmarkbao@gmail.com>",
  "files": [
    "dist",
    "bin"
  ],
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "bin": {
    "ni": "bin/ni.js",
    "nci": "bin/nci.js",
    "nr": "bin/nr.js",
    "nu": "bin/nu.js",
    "nx": "bin/nx.js",
    "nrm": "bin/nrm.js",
    "test": "bin/test.js",
    "testTpl": "bin/tpl.js"
  },
  "bugs": {
    "url": "https://github.com/bookmarkbao/tool-123.git/issues"
  },
  "homepage": "https://github.com/bookmarkbao/tool-123.git#readme",
  "scripts": {
    "prepublishOnly": "rimraf dist && npm run build",
    "watch:tpl": "npm run build:tpl -- --watch",
    "watch": "npm run build -- --watch",
    "?ni": "npm install",
    "ni": "esno src/ni.ts",
    "nci": "esno src/nci.ts",
    "nr": "esno src/nr.ts",
    "nu": "esno src/nu.ts",
    "nx": "esno src/nx.ts",
    "nrm": "esno src/nrm.ts",
    "t123": "esno src/test.ts",
    "tpl": "esno src/tpl.ts",
    "dev": "esno src/ni.ts",
    "build:tpl": "rimraf dist/tpl* && tsup src/tpl.ts --format cjs,esm --dts",
    "build": "rimraf dist && tsup src/ni.ts src/nci.ts src/nr.ts src/nu.ts src/nx.ts src/nrm.ts src/index.ts src/test.ts src/tpl.ts --format cjs,esm --dts",
    "release": "npx git-ensure && npx bumpp --commit --push --tag",
    "lint": "eslint \"**/*.ts\"",
    "lint:fix": "npm run lint -- --fix",
    "test": "c8 ava"
  },
  "devDependencies": {
    "@antfu/eslint-config-ts": "^0.7.0",
    "@types/ini": "^1.3.30",
    "@types/node": "^16.7.10",
    "@types/prompts": "^2.4.0",
    "ava": "^3.15.0",
    "c8": "^7.8.0",
    "chalk": "^4.1.2",
    "esbuild-register": "^3.0.0",
    "eslint": "^7.32.0",
    "esm": "^3.2.25",
    "esno": "^0.9.1",
    "execa": "^5.1.1",
    "find-up": "^5.0.0",
    "fs-extra": "^10.0.0",
    "ini": "^2.0.0",
    "nodemon": "^2.0.14",
    "prompts": "^2.4.1",
    "rimraf": "^3.0.2",
    "swig": "^1.4.2",
    "terminal-link": "^3.0.0",
    "ts-node": "^10.4.0",
    "tsup": "^4.14.0",
    "typescript": "^4.4.2"
  },
  "eslintConfig": {
    "extends": "@antfu/eslint-config-ts"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "esm",
      "esbuild-register"
    ]
  },
  "gitHead": "aca2fcc53440041264885cdd2f0ab805d23b4bc0"
}
